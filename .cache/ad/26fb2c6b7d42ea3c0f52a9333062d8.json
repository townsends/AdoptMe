original":{"line":71,"column":15}},{"generated":{"line":90,"column":8},"source":"SearchParams.js","original":{"line":71,"column":19}},{"name":"pets","generated":{"line":90,"column":10},"source":"SearchParams.js","original":{"line":71,"column":21}},{"generated":{"line":91,"column":0},"source":"SearchParams.js","original":{"line":71,"column":6}},{"generated":{"line":91,"column":4},"source":"SearchParams.js","original":{"line":38,"column":4}},{"generated":{"line":91,"column":5},"source":"SearchParams.js","original":{"line":37,"column":2}},{"generated":{"line":92,"column":0},"source":"SearchParams.js","original":{"line":74,"column":1}},{"generated":{"line":92,"column":1},"source":"SearchParams.js","original":{"line":7,"column":0}},{"name":"SearchParams","generated":{"line":94,"column":15},"source":"SearchParams.js","original":{"line":76,"column":15}}],"sources":{"SearchParams.js":"import React, { useState, useEffect, useContext } from \"react\";\r\nimport pet, { ANIMALS } from \"@frontendmasters/pet\";\r\nimport useDropdown from \"./useDropdown\";\r\nimport Results from \"./Results\";\r\nimport ThemeContext from \"./ThemeContext\";\r\n\r\nconst SearchParams = () => {\r\n  const [theme, setTheme] = useContext(ThemeContext);\r\n  const [location, updateLocation] = useState(\"Seattle, WA\");\r\n  const [breeds, updateBreeds] = useState([]);\r\n  const [pets, setPets] = useState([]);\r\n  const [animal, AnimalDropdown] = useDropdown(\"Animal\", \"dog\", ANIMALS);\r\n  const [breed, BreedDropdown, updateBreed] = useDropdown(\"Breed\", \"\", breeds);\r\n\r\n  async function requestPets() {\r\n    const { animals } = await pet.animals({\r\n      location,\r\n      breed,\r\n      type: animal\r\n    });\r\n\r\n    console.log(\"animals\", animals);\r\n\r\n    setPets(animals || []);\r\n  }\r\n\r\n  useEffect(() => {\r\n    updateBreeds([]);\r\n    updateBreed(\"\");\r\n\r\n    pet.breeds(animal).then(({ breeds }) => {\r\n      const breedStrings = breeds.map(({ name }) => name);\r\n      updateBreeds(breedStrings);\r\n    }, console.error);\r\n  }, [animal]);\r\n\r\n  return (\r\n    <div className=\"search-params\">\r\n      <form\r\n        onSubmit={e => {\r\n          e.preventDefault();\r\n          requestPets();\r\n        }}\r\n      >\r\n        <label htmlFor=\"location\">\r\n          Location\r\n          <input\r\n            id=\"location\"\r\n            value={location}\r\n            placeholder=\"Location\"\r\n            onChange={e => updateLocation(e.target.value)}\r\n          />\r\n        </label>\r\n        <AnimalDropdown />\r\n        <BreedDropdown />\r\n        <label htmlFor=\"location\">\r\n          Theme\r\n          <select\r\n            value={theme}\r\n            onChange={e => setTheme(e.target.value)}\r\n            onBlur={e => setTheme(e.target.value)}\r\n          >\r\n            <option value=\"peru\">Peru</option>\r\n            <option value=\"darkblue\">Dark Blue</option>\r\n            <option value=\"chartreuse\">Chartreuse</option>\r\n            <option value=\"mediumorchid\">Medium Orchid</option>\r\n          </select>\r\n        </label>\r\n        <button style={{ backgroundColor: theme }}>Submit</button>\r\n      </form>\r\n      <Results pets={pets} />\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default SearchParams;"},"lineCount":null}},"error":null,"hash":"ce54967b2a820783039a9c97264b149a","cacheData":{"env":{}}}